# OpenAI Configuration (Nutrition Agent)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo
OPENAI_TEMPERATURE=0.7
OPENAI_MAX_TOKENS=2000

# Agent Configuration
AGENT_MAX_ITERATIONS=10
AGENT_TIMEOUT=60
AGENT_VERBOSE=true

# Nutrition Database APIs (Optional)
NUTRITIONIX_API_KEY=your_nutritionix_api_key
NUTRITIONIX_APP_ID=your_nutritionix_app_id
USDA_API_KEY=your_usda_api_key

# Integration Endpoints
PHOTO_ANALYSIS_API=http://localhost:7000/api/photoanalysis
WHOOP_API=http://localhost:7000/api/whoop

# Meal Planning Defaults
DEFAULT_MEALS_PER_DAY=3
DEFAULT_PROTEIN_RATIO=0.30
DEFAULT_CARB_RATIO=0.40
DEFAULT_FAT_RATIO=0.30

# Application
APP_ENV=development
DEBUG=true
